#include <stdio.h>

int main() {
   int rows,i,j,k;
   printf("enter no of rows:");
   scanf("%d",&rows);
   for(i=1;i<=rows;i++)
   {
       for(k=0;k<i;k++)
       {
           printf(" ");
       }
       for(j=rows;j>=i;j--){
           printf("*");
       }   
      
       printf("\n");
   }
    return 0;
}
